{
    "compilerOptions": {
        "types": [
            "node"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "outDir": "lib",
        "target": "esnext",
        "module": "CommonJS",
        "lib": [
            "es2019"
        ],
        "skipLibCheck": true,
        "strict": true,
        "declaration": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "resolveJsonModule": true,
        "downlevelIteration": true,
        "sourceMap": true
    }
}